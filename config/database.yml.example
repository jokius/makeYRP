default: &default
  adapter: postgresql
  encoding: unicode
  pool: <%= ENV.fetch("RAILS_MAX_THREADS") { 5 } %>
  host: postgres
  username: postgres
  password: postgres

development:
  <<: *default
  database: make_yrp_development

test:
  <<: *default
  database: make_yrp_test

production:
  <<: *default
  database: make_yrp_production
